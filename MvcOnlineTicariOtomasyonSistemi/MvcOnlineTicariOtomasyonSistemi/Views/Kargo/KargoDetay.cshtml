@using MvcOnlineTicariOtomasyonSistemi.Models.Siniflar
@model List<KargoTakip>
@{
    ViewBag.Title = "KargoDetay";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<main>
    <div class="container-fluid">
        <h1 class="mt-4"></h1>
        <ol class="breadcrumb mb-4">
            <li class="breadcrumb-item active">Kargo Takip</li>
        </ol>
        <div class="card mb-4">
            <div class="card-header">
                <i class="fas fa-table mr-1"></i>
                Kargo Durum Listesi

            </div>
            <div class="card-body">
                <div class="table-responsive">

                    <table class="table table-bordered" width="100%" cellspacing="0">
                        <thead>
                            <tr>
                                <th>AÇIKLAMA</th>
                                <th>TARİH</th>                                
                            </tr>
                        </thead>
                        <tfoot>
                            <tr>
                                <th>AÇIKLAMA</th>
                                <th>TARİH</th>
                            </tr>
                        </tfoot>
                        <tbody>
                            @foreach (var k in Model)
                            {
                                <tr>
                                    <td>@k.Aciklama</td>
                                    <td>@k.Tarih</td>                                   
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</main>





